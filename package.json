{
  "author": "Assad Khan",
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "My solutions to Advent of Code",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:2015": "vitest --dir src/2015",
    "test:2016": "vitest --dir src/2016",
    "test:2017": "vitest --dir src/2017",
    "test:2018": "vitest --dir src/2018",
    "test:2019": "vitest --dir src/2019",
    "test:2020": "vitest --dir src/2020",
    "test:2021": "vitest --dir src/2021",
    "test:2022": "vitest --dir src/2022",
    "test:2023": "vitest --dir src/2023",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "scratch": "node scratch.js"
  },
  "keywords": [
    "advent-of-code",
    "aoc",
    "2015",
    "2016",
    "2017",
    "2018",
    "2020",
    "2021",
    "2022",
    "2023",
    "2024",
    "javascript"
  ],
  "devDependencies": {
    "@assadk/aoc-utils": "^1.6.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^7.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "vitest": "^2.1.8"
  }
}
